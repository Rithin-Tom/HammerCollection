<style>
  /* Ensure the carousel controls are vertically centered */
.carousel-control-prev,
.carousel-control-next {
  top: 50%;
  transform: translateY(-50%);
}

/* Optional: make controls always visible (not fade based on hover) */
.carousel-control-prev,
.carousel-control-next {
  opacity: 1;
}

.seeking-section {
  background: black, url('https://i.pinimg.com/originals/68/64/e2/6864e296b6a5b9c4aa2a42974cb8ab85.jpg') no-repeat center center;
  background-size: cover;
  min-height: 60vh;
  color: white;
  position: relative;
  overflow: hidden;
}

.seeking-highlight {
  color: #FFD700;
  text-shadow: 0 0 10px #000;
}

.seeking-text {
  max-width: 700px;
  padding: 1rem;
}

.seeking-overlay {
  position: absolute;
  top: 0; left: 0;
  width: 100%; height: 100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 1;
}



